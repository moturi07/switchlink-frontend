        try {\r\n                    let res = await axios({\r\n                        url: \"https://localhost:44325/api/Accounts/GetAccount/1\",\r\n                        method: \"GET\",\r\n                    });\r\n                    let trans = await axios({\r\n                        url: \"https://localhost:44325/api/values/Get/1111\",\r\n                        method: \"GET\",\r\n                    });\r\n                    let accounts = JSON.parse(res.data);\r\n                    let transactions= JSON.parse(trans.data);\r\n                    this.accounts = accounts;\r\n                    this.transactions=transactions;\r\n                    this.loading = false;\r\n                } catch (error) {\r\n                    this.loading = false;\r\n                }\r\n            },\r\n        },\r\n    };\r\n</script>\r\n<style>\r\n    .home {\r\n        padding: 0 30px;\r\n        max-width: 800px;\r\n        margin: 0 auto;\r\n    }\r\n    @keyframes blink {\r\n        from {\r\n            opacity: 1;\r\n        }\r\n        to {\r\n            opacity: 0;\r\n        }\r\n    }\r\n    .post--empty {\r\n        height: 250px;\r\n        margin-top: 30px;\r\n        animation: blink 0.8s ease-in-out infinite alternate both;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n        font-family: \"Lobster\", cursive;\r\n    }\r\n    ul {\r\n        text-align: left;\r\n    }\r\n    a {\r\n        color: inherit;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}