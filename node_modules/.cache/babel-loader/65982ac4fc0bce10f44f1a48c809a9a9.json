ng....</p>\r\n        <div class=\"row\" v-else>\r\n            <div class=\"col-12 col-md-6 col-lg-6\" v-for=\"account in accounts\" :key=\"account.id\">\r\n                {{account.first_name}}\r\n                {{account.account_number}}\r\n                {{account.balance}}\r\n                <div class=\"col-12\">\r\n                    <form @submit.prevent=\"handleDeposit\">\r\n                        <h2>Deposit</h2>\r\n                        <div class=\"mb-3\">\r\n                            <input type=\"hidden\" class=\"form-control\" v-model=\"user_id\" >\r\n                            <input type=\"hidden\" class=\"form-control\" v-model=\"account_number\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label for=\"deposit_amount\" class=\"form-label\">Amount</label>\r\n                            <input type=\"number\" class=\"form-control\" v-model=\"deposit_amount\" id=\"deposit_amount\">\r\n                        </div>\r\n                        <button class=\"btn btn-primary\">Deposit Funds</button>\r\n                    </form>\r\n                </div>\r\n                <div class=\"col-12\">\r\n                    <form @submit.prevent=\"handleWithdrawal\">\r\n                        <h2>Withdraw Funds</h2>\r\n                        <div class=\"mb-3\">\r\n                            <input type=\"hidden\" class=\"form-control\" v-model=\"user_id\">\r\n                            <input type=\"hidden\" class=\"form-control\" v-model=\"account_number\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label for=\"withdraw_amount\" class=\"form-label\">Amount</label>\r\n                            <input type=\"number\" class=\"form-control\" v-model=\"withdraw_amount\" id=\"withdraw_amount\">\r\n                        </div>\r\n                        <button class=\"btn btn-primary\">Withdraw Funds</button>\r\n                    </form> \r\n                </div>\r\n                <div class=\"col-12\">\r\n                    <form @submit.prevent=\"handleTransfer\">\r\n                        <h2>Transfer Funds</h2>\r\n                        <div class=\"mb-3\">\r\n                            <input type=\"hidden\" class=\"form-control\" v-model=\"user_id\">\r\n                            <input type=\"hidden\" class=\"form-control\" v-model=\"account_number\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label for=\"receiver_account\" class=\"form-label\">Receiver Account</label>\r\n                            <input type=\"number\" class=\"form-control\" v-model=\"receiver_account\" id=\"receiver_account\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label for=\"transfer_amount\" class=\"form-label\">Amount</label>\r\n                            <input type=\"number\" class=\"form-control\" v-model=\"transfer_amount\" id=\"transfer_amount\">\r\n                        </div>\r\n                        <button class=\"btn btn-primary\">Transfer Funds</button>\r\n                    </form> \r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"col-12 col-md-6 col-lg-6\">\r\n                <h2>Transactions</h2>\r\n                <ul>\r\n                    <li v-for=\"records in transactions\" :key=\"records.id\">                \r\n                        {{ records.transaction_type }}\r\n                        {{ records.amount }}\r\n                        {{ records.date }}\r\n                    </li>\r\n                </ul>\r\n            </div>\r\n        </div>\r\n        \r\n    </div>\r\n</template>\r\n<script>\r\n// @ is an alias to /src\r\nimport axios from \"axios\";\r\nimport firebase from 'firebase/app';\r\n\r\n// These imports load individual services into the firebase namespace.\r\nimport 'firebase/auth';\r\nimport 'firebase/database';   \r\n\r\n    export default {\r\n        data() {\r\n            return { \r\n                user_id: null,\r\n                account_number: null,\r\n                deposit_amount: null,\r\n                withdraw_amount: null,\r\n                transfer_amount: null,\r\n                receiver_account: null,\r\n                userName: null,\r\n                transactions: null,\r\n                accounts: null,\r\n                loading: false,\r\n                \r\n            };\r\n        },       \r\n\r\n        mounted() {\r\n            this.getPosts();\r\n        },\r\n        \r\n        methods: {            \t\r\n            async getPosts() {\r\n                var user=firebase.auth().currentUser;\r\n                if(user !=null){\r\n                    var hash = md5(user.email);\r\n                    this.loading = true;\r\n                    try {\r\n                        let res = await axios({\r\n                            url: \"https://localhost:44325/api/Accounts/GetAccount/\"+hash,\r\n                            method: \"GET\",\r\n                        });\r\n                        let trans = await axios({\r\n                            url: \"https://localhost:44325/api/values/Get/1111\",\r\n                            method: \"GET\",\r\n                        });\r\n                        let accounts = JSON.parse(res.data);\r\n                        let transactions= JSON.parse(trans.data);\r\n                        this.accounts = accounts;\r\n                        this.transactions=transactions;\r\n\r\n                        this.user_id=accounts[0].id;\r\n                        this.account_number=accounts[0].account_number;\r\n                        this.loading = false;\r\n                    } catch (error) {\r\n                        this.loading = false;\r\n                    }\r\n                }\r\n                else{\r\n                    await firebase.auth().signOut();\r\n                    this.$router.push('/login');\r\n                }\r\n            },\r\n            async handleDeposit(){\r\n                try{\r\n                    const response=await axios.post('Accounts/MakeDeposit/'+this.user_id, {\r\n                        account_number: this.account_number,\r\n                        deposit_amount: this.deposit_amount\r\n                    });        \r\n                    console.log(response);\r\n                    this.getPosts();\r\n                    //this.$router.replace({name:'dashboard'});\r\n                }\r\n                catch(err){\r\n                    console.log(err)\r\n                }\r\n            },\r\n            async handleWithdrawal(){\r\n                try{\r\n                    const response=await axios.post('Accounts/MakeWithdrawal/'+this.user_id, {\r\n                        account_number: this.account_number,\r\n                        withdraw_amount: this.withdraw_amount\r\n                    });        \r\n                    console.log(response);\r\n                    this.getPosts();\r\n                }\r\n                catch(err){\r\n                    console.log(err)\r\n                }\r\n            },\r\n            async handleTransfer(){\r\n                try{\r\n                    const response=await axios.post('Accounts/TransferFunds/'+this.user_id, {\r\n                        account_number: this.account_number,\r\n                        receiver_account: this.receiver_account,\r\n                        transfer_amount: this.transfer_amount\r\n                    });        \r\n                    console.log(response);\r\n                    this.getPosts();\r\n                }\r\n                catch(err){\r\n                    console.log(err)\r\n                }\r\n            }\r\n        },\r\n    };\r\n</script>\r\n<style>\r\n    .home {\r\n        padding: 0 30px;\r\n        max-width: 800px;\r\n        margin: 0 auto;\r\n    }\r\n    @keyframes blink {\r\n        from {\r\n            opacity: 1;\r\n        }\r\n        to {\r\n            opacity: 0;\r\n        }\r\n    }\r\n    .post--empty {\r\n        height: 250px;\r\n        margin-top: 30px;\r\n        animation: blink 0.8s ease-in-out infinite alternate both;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n        font-family: \"Lobster\", cursive;\r\n    }\r\n    ul {\r\n        text-align: left;\r\n    }\r\n    a {\r\n        color: inherit;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}